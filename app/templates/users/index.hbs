<h1>List of users</h1>
<table class="table table-striped">
    <tr>
        <th>id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Login</th>
        <th>Email</th>
        <th>Created</th>
        <th>Updated</th>
        <th>Actions</th>
    </tr>
    {{#each models}}
        <tr>
            <td>{{id}}</td>
            <td>{{login}}</td>
            <td>{{firstName}}</td>
            <td>{{lastName}}</td>
            <td>{{email}}</td>
            <td>{{dateFormat created}}</td>
            <td>{{dateFormat updated}}</td>
            <td>
                <div class="btn-group btn-group-xs">
                    <a href="/users/edit/{{id}}" class="btn btn-default"><span class="glyphicon glyphicon-edit"></span></a>
                    <a href="#" class="btn btn-default" data-remove-id="{{id}}"><span class="glyphicon glyphicon-remove"></span></a>
                </div>
            </td>
        </tr>
    {{/each}}
</table>
<div class="pull-right">
    {{view "pagination" pagination=meta url='/users/:page'}}
</div>